<Window x:Class="WindowsPerformanceMonitor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:tabData="clr-namespace:WindowsPerformanceMonitor"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" ResizeMode="NoResize">
    <TabControl>
        <TabItem Header="Real Time">
            <tabData:RealTime x:Name="ucTab1Data1" Margin="0,-5,0,5" />
        </TabItem>
        <TabItem Header="Logging">
            <tabData:Logging x:Name="ucTab1Data2" />
        </TabItem>
        <TabItem Header="Hardware Details">
            <tabData:HardwareDetails x:Name="ucTab1Data3" />
        </TabItem>
        <TabItem Header="Startup Apps">
            <tabData:Startup x:Name="ucTab1Data4" />
        </TabItem>
        <TabItem Header="Options">
            <tabData:Options x:Name="ucTab1Data5" />
        </TabItem>
    </TabControl>
</Window>
